{
  "name": "parcel-vue-js",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/",
    "dev": "parcel serve index.html --open",
    "build": "npm run clean && NODE_ENV=production ./build.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie 11",
    "not ie_mob 11",
    "not op_mini all",
    "not op_mob > 0",
    "not and_qq > 0",
    "not android > 0"
  ],
  "postcss": {
    "plugins": {
      "postcss-media-minmax": {},
      "postcss-nested": {},
      "postcss-hexrgba": {},
      "postcss-pxtorem": {
        "propList": [
          "*"
        ],
        "selectorBlackList": [
          "html"
        ],
        "rootValue": 16
      },
      "autoprefixer": {}
    }
  },
  "staticPath": "public",
  "engines": {
    "node": "^11.0.0"
  },
  "browser": {},
  "dependencies": {
    "vue": "^2.5.16",
    "vue-hot-reload-api": "^2.3.0"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^1.1.0",
    "autoprefixer": "^9.4.8",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "css-mqpacker": "^7.0.0",
    "parcel-bundler": "^1.7.1",
    "postcss": "^7.0.16",
    "postcss-hexrgba": "^1.0.1",
    "postcss-media-minmax": "^4.0.0",
    "postcss-nested": "^4.1.1",
    "postcss-pxtorem": "^4.0.1",
    "posthtml": "^0.11.4",
    "vue-template-compiler": "^2.5.16"
  }
}
